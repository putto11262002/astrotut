{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "astro/tsconfigs/strict",
	"include": [
		".astro/types.d.ts",
		"**/*"
	],
	"exclude": [
		"dist"
	],
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "react",
		"baseUrl": ".",
		"strict": true,
		"paths": {
			"@/*": [
				"./src/*"
			]
		}
	}
}
